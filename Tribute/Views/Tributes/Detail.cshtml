@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>Tribute to @ViewBag.TributeTitle</h1>
        <h5>Description:</h5>
        <div>@Html.Raw(ViewBag.Description)</div>
        @if (ViewBag.Artists.Length > 0)
        {
            <div>
                <ul>
                    @foreach (var artist in ViewBag.Artists)
                {
                        <text>
                            <li>@artist</li> </text>
                    }
                </ul>
            </div>
        }
        
    </div>
</body>
</html>
